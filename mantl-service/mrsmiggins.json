{
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": true,
      "network": "BRIDGE",
      "image": "mattsday/mrsmiggins:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  },
  "id": "mrsmiggins-website",
  "instances": 5,
  "cpus": 0.1,
  "mem": 128
}
